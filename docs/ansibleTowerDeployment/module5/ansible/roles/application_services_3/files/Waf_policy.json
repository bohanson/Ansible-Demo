{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "historyLimit": 10,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.0.0",
    "id": "8316914",
    "label": "MyTest",
    "remark": "Simple HTTP application referencing an externally hosted iRule",
    "MyTest": {
      "class": "Tenant",
      "A1": {
        "class": "Application",
        "template": "https",
        "serviceMain": {
          "class": "Service_HTTPS",
          "virtualAddresses": [
            "10.1.10.171"
          ],
          "snat": "auto",
          "serverTLS": "webtls",
          "pool": "web_pool2",
          "policyWAF": {
          "use": "My_ASM_Policy"
          }
        },
        "web_pool2": {
          "class": "Pool",
          "loadBalancingMode": "predictive-node",
          "monitors": [
            "http"
          ],
          "members": [{
            "servicePort": 8080,
            "serverAddresses": [
              "10.1.20.11"
            ]
          }]
        },
        "My_ASM_Policy": {
        "class": "WAF_Policy",
        "url": "https://raw.githubusercontent.com/kreynoldsf5/ASE-AS3-Exercise/master/WAF-policy.xml",
        "ignoreChanges": true
        }
      }
    }
  }
}
